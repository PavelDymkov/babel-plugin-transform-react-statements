    "jsx",
    "statement",
    "for",
    "forEach",
    "if",
    "switch"
  ],
  "author": "Pavel Dymkov <dymkov86@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "babel-plugin-test-helper": "git+https://github.com/PavelDymkov/babel-plugin-test-helper.git",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2"
  }
}
