{
  "name": "babel-plugin-transform-react-statements",
  "version": "1.0.0",
  "description": "Transform React statements",
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && babel src --out-dir lib",
    "dev": "rimraf lib && babel src --out-dir lib --source-maps --watch",
    "prepublish": "npm run build",
    "test": "rimraf lib && npm run build && mocha \"./test/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PavelDymkov/babel-plugin-transform-react-statements.git"
  },
  "keywords": [
